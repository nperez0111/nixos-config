#!/bin/sh -e

VERSION=1.0

GREEN='\033[1;32m'
RED='\033[1;31m'
CLEAR='\033[0m'

# Navigate to the directory of this script
cd $(dirname $(readlink -f $0))
cd ..

echo "${GREEN}Backing up existing configurations...${CLEAR}"
echo -ne "Ryujinx..."
zip -r ./configs/ryujinx.zip "$HOME/Library/Application Support/Ryujinx"
echo "${GREEN}DONE${CLEAR}"
